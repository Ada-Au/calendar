{
  "name": "yc2-email",
  "version": "0.2.0",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "setup": "cp .env-sample .env",
    "start": "NODE_ENV=production node dist/index.js",
    "clean": "rm -rf dist",
    "prebuild": "npm -s run clean && npm -s run generate:nexus",
    "build": "tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "npx prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "dev": "NODE_ENV=development ts-node-dev --transpile-only --no-notify src/index",
    "dev:typecheck": "tsc --noEmit --watch"
  },
  "dependencies": {
    "@apollo/federation": "^0.20.7",
    "@prisma/client": "2.17.0",
    "@ra-data-prisma/backend": "^7.2.0",
    "@sendgrid/mail": "^7.4.1",
    "apollo-server": "^2.19.0",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "graphql": "^15.4.0",
    "graphql-middleware": "^4.0.3",
    "graphql-shield": "^7.4.4",
    "graphql-transform-federation": "^2.2.0",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.35.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "^14.18.13",
    "dotenv": "^8.2.0",
    "prisma": "2.17.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
